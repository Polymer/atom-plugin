{
  "name": "polymer-ide",
  "main": "./lib/polymer-ide",
  "version": "0.2.0",
  "description": "Provides linting, autocompletion, and more for web components.",
  "keywords": [],
  "repository": "https://github.com/polymer/atom-plugin",
  "license": "BSD-3-Clause",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "preversion": "tsc && git add lib/ -f",
    "postversion": "git rm -r lib/ && git commit -m \"Automatic removal of lib folder after release commit.\" && tsc"
  },
  "dependencies": {
    "@types/node": "^6.0.41",
    "marked": "^0.3.6",
    "polymer-analyzer": "=2.0.0-alpha.16",
    "polymer-editor-service": "^1.1.0"
  },
  "devDependencies": {
    "@types/atom": "0.0.32",
    "@types/marked": "0.0.28",
    "tslint": "^3.15.1",
    "typescript": "^2.0.3"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    },
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocompleter"
      }
    }
  }
}
